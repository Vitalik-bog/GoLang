package main

import "fmt"

func main() {
	defer fmt.Println("defer 1") //3
	fmt.Println("начало")

	defer func() {
		fmt.Println("defer 2") //2
		panic("вторая паника!")
	}()
	defer func() {
		fmt.Println("defer 3") //1
		panic("3 паника!")
	}()

	panic("первая паника!")

	defer fmt.Println("defer 3 - никогда не выполнится")
}
