package main

import "fmt"

func main() {
    numbers := make(chan int)
    squares := make(chan int)

    // Горутина для вычисления квадратов
    go func() {
        for num := range numbers {
            squares <- num * num
        }
        close(squares)
    }()

    // Главная горутина отправляет числа
    go func() {
        for i := 1; i <= 5; i++ {
            numbers <- i
        }
        close(numbers)
    }()

    // Главная горутина выводит квадраты
    for square := range squares {
        fmt.Println(square)
    }
}
