package main

import "fmt"

type User struct {
    name     string
    age      int64
    password string
}

func (u User) isElder() bool {
    a := u.age 
    isTrue := false

    if a >= 18 {
        isTrue = true
    } else if a < 18 {  
        isTrue = false
    }
    return isTrue
}

func main() {
    user := User{"John", 23, "pass"}  
    if user.isElder(){
      fmt.Println("Добро пожаловать в бойцовский клуб!")
    }else{
      fmt.Println("Нельзя!Проваливай!")
    }
}
