package main

import (
	"fmt"
)

func writer() <-chan int {
	ch := make(chan int)

	go func() {

		for i := 0; i < 5; i++ {
			ch <- i + 1
		}

		for i := 0; i < 5; i++ {
			ch <- i + 11
		}
		close(ch)
	}()

	return ch
}

func main() {
	ch := writer()

	for v := range ch {
		fmt.Println("v=", v)
	}
}
