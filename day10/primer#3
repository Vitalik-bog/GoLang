package main

import "fmt"

func main() {
    name := "Анна"
    age := 25
    height := 1.65
    
    // %s - для строк
    // %f - для дробных чисел
    // %.2f - для дробных с 2 знаками после запятой
    fmt.Printf("Имя: %s, Возраст: %d, Рост: %.2f\n", name, age, height)
    // Выведет: Имя: Анна, Возраст: 25, Рост: 1.65
}
