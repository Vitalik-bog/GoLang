package main

import "fmt"

func main() {
    name := "John"
    
    switch name {
    case "Jordan":
        fmt.Println("Hello Jordan")
    case "Kate":
        fmt.Println("Hello Kate")
    case "John":
        fmt.Println("Hello John")
    default:
        fmt.Println("Hello stranger!") // Выполнится если имя не совпадает
    }
}
