package main

import "fmt"

func main() {
    currency := "EUR"
    
    switch currency {
    case "USD":
        fmt.Println("Доллар США")
    case "EUR":
        fmt.Println("Евро")
    case "RUB":
        fmt.Println("Российский рубль")
    default:
        fmt.Println("Неизвестная валюта")
    }
}
