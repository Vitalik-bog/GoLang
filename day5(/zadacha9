package main

import "fmt"

func main() {
    var value interface{} = 2
    
    switch v := value.(type) {
    case int:
        fmt.Printf("Это число: %d\n", v)
    case string:
        fmt.Printf("Это строка: %s\n", v)
    case bool:
        fmt.Printf("Это булево значение: %t\n", v)
    default:
        fmt.Println("Неизвестный тип")
    }
}
// Type switch - мощная фича Go для определения типов
