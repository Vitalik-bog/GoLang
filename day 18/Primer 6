package main

import "fmt"

func main() {
    num1 := 5
    num2 := 5
    
    ptr1 := &num1
    ptr2 := &num2
    ptr3 := &num1
    
    // Сравни указатели и выведи true/false
    fmt.Println("ptr1 == ptr2:", fmt.Println("ptr1 == ptr2:", ptr1 == ptr2)) // false 
    fmt.Println("ptr1 == ptr3:", fmt.Println("ptr1 == ptr3:", ptr1 == ptr3)) // true
}
