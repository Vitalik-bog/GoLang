package main

import "fmt"

func main() {
    colors := map[string]string{
        "red":   "#FF0000",
        "green": "#00FF00",
        "blue":  "#0000FF",
    }
    
    if _, exists := colors["yellow"]; 
    exists {
        fmt.Println("Цвет найден")
    } else {
        fmt.Println("Цвет не найден")
    }
}
