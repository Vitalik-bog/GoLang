package main

import "fmt"

func main() {
	var input string

	fmt.Print("Введите пожалуйста строку: ")
	fmt.Scanln(&input)

	length := len(input)
	fmt.Printf("Длинна строки равна '%s' :%d\n", input, length)
}

package main

import "fmt"

func main() {
	stroka := "hello,world"
	length := len(stroka)

	fmt.Printf("Строка\"%s\"\n", stroka)
	fmt.Printf("Длина строки: %d символов\n", length)
}
