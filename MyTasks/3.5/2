package main

import "fmt"

func main() {
    arr := []int{1, 2, 3, 4, 5, 6}
    
    allPositive := true
    
    for _, num := range arr {
        if num <= 0 {
            allPositive = false
            break
        }
    }
    
    if allPositive {
        fmt.Println("Все элементы среза являются положительными числами")
    } else {
        fmt.Println("Не все элементы среза являются положительными числами")
    }
}
