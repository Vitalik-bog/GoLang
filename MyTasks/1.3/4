package main

import "fmt"

func main() {
    num := 1 // можно изменить на любой от 1 до 12
    
    var season string
    
    switch num {
    case 12, 1, 2:
        season = "Зима"
    case 3, 4, 5:
        season = "Весна"
    case 6, 7, 8:
        season = "Лето"
    case 9, 10, 11:
        season = "Осень"
    default:
        season = "Неверный номер месяца"
    }
    
    fmt.Println(season)
}
