package main

import "fmt"

func main() {
    mb := 35
    
    // 1 МБ = 1024 * 1024 байт
    bytes := mb * 1024 * 1024
    
    fmt.Printf("%d МБ = %d байт\n", mb, bytes)
    
    // Альтернативное решение с использованием сдвига битов
    bytes2 := mb << 20 // 2^20 = 1048576 (1 МБ в байтах)
    fmt.Printf("%d МБ = %d байт (через сдвиг)\n", mb, bytes2)
}
