package main

import (
    "fmt"
    "strconv"
)

func main() {
    str1 := "123"
    str2 := "456" 
    str3 := "789"
    
    num1, _ := strconv.Atoi(str1)
    num2, _ := strconv.Atoi(str2)
    num3, _ := strconv.Atoi(str3)
    
    sum := num1 + num2 + num3
    fmt.Printf("%s + %s + %s = %d\n", str1, str2, str3, sum)
}
