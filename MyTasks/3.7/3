package main

import (
    "fmt"
    "strconv"
)

func main() {
    slc := [5]float64{1.456, 2.125, 3.32, 4.1, 5.34}
    fmt.Print("Округленные дроби: ")
    for _, num := range slc {
        // Округляем до одного знака
        rounded := fmt.Sprintf("%.1f", num)
        fmt.Printf("%s ", rounded)
    }
    fmt.Println()
}
