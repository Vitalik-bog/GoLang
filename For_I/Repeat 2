package main

import "fmt"

func main() {
    numbers := []int{3, 7, 2, 9, 4, 1}
    max := numbers[0] 
    
    for i := 1; i < len(numbers); i++ {
        if numbers[i] > max {
            max = numbers[i]
        }
    }
    
    fmt.Println("Максимальное число:", max)
    fmt.Println("Максимальное число:", max)
}
